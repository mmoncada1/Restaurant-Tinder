{
	"version": "2.0.0",
	"tasks": [
		{
			"label": "Build iOS App",
			"type": "shell",
			"command": "xcodebuild",
			"args": [
				"-project",
				"RestaurantSwipe.xcodeproj",
				"-scheme",
				"RestaurantSwipe",
				"-configuration",
				"Debug",
				"-sdk",
				"iphonesimulator",
				"-destination",
				"platform=iOS Simulator,name=iPhone 15 Pro",
				"build"
			],
			"group": "build",
			"problemMatcher": [
				"$gcc"
			],
			"isBackground": false
		}
	]
}